{
	"name": "sharepage",
	"version": "1.5.0",
	"description": "Share notes via GitHub repository",
	"main": "main.js",
	"scripts": {
		"dev": "vite build --watch",
		"build": "tsc -noEmit -skipLibCheck && vite build",
		"check": "tsc --noEmit --skipLibCheck",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"github",
		"share"
	],
	"author": "wis-graph",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.11.0",
		"builtin-modules": "^3.3.0",
		"obsidian": "latest",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.0"
	},
	"dependencies": {
		"octokit": "^4.0.0"
	}
}
